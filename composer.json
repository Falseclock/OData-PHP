{
  "name":         "falseclock/odata-server-php",
  "type":         "library",
  "description":  "OData PHP Application server",
  "keywords":     [
    "OData",
    "server",
    "REST",
    "API"
  ],
  "license":      "MIT",
  "authors":      [
    {
      "name":  "Nurlan Mukhanov",
      "email": "nurike@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/Falseclock/dbd-php"
    }
  ],
  "require":      {
    "php":                       "^7.1",
    "ext-json":                  "*",
    "ext-xmlwriter":             "*",
    "falseclock/dbd-php-entity": "^1.0",
    "falseclock/dbd-php":        "dev-RealPrepareExecute"
  },
  "require-dev":  {
    "symfony/var-dumper": "^3.3"
  },
  "suggest":      {
  },
  "autoload":     {
    "psr-4": {
      "Falseclock\\OData\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config":       {
    "optimize-autoloader": true,
    "apcu-autoloader":     true
  }
}